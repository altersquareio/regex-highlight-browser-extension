{
	"manifest_version": 3,
	"name": "Regex Highlighter",
	"version": "5.1",
	"description": "Highlights text on a page using regular expressions.",
	"permissions": [
		"activeTab",
		"storage",
		"scripting"
	],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {
		"default_popup": "src/popup.html"
	}
}
